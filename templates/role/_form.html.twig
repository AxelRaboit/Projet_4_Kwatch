{{ form_start(form) }}
    <div class="containerNewRoleForm">
        <div class="containerRoleFormName">
            <div class="itemRoleFormName">{{ form_row(form.name) }}</div>
        </div>
        <div class="containerRoleFormActor">
            <div class="itemRoleFormActor">{{ form_row(form.actor) }}</div>
        </div>
        <div class="containerRoleFormProgram">
            <div class="itemRoleFormProgram">{{ form_row(form.program) }}</div>
        </div>
        <div class="containerNewRoleButtons">
            <button class="itemRoleSubmitButton">{{ button_label|default('Cr√©er') }}</button>
        </div>
    </div>
{{ form_end(form) }}




